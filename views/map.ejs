    <!-- script -->
    
    <script>
        var atribution = "Â© OpenStreetMap contributors";
        var mymap = L.map('map').setView([10.0442,76.3282], 14.5);
        var tileURL = "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png";
        var dhishnaIcon = L.icon({
            iconUrl: '/assets/img/map/dhishna-icon.png',
            iconSize:     [40, 40], // size of the icon
            shadowSize:   [50, 64], // size of the shadow
            iconAnchor:   [20, 40], // point of the icon which will correspond to marker's location
            shadowAnchor: [4, 62],  // the same for the shadow
            popupAnchor:  [0,-40] // point from which the popup should open relative to the iconAnchor
        });
        
        const tiles = L.tileLayer(tileURL,{atribution});
        tiles.addTo(mymap);
        var marker = L.marker([10.04208, 76.32815]).addTo(mymap);
        marker.bindPopup("<b>CUSAT</b><br> <a href=\"https://goo.gl/maps/cHxYFkPUGmSTpaCz6\">Show in Maps</a>").openPopup();
        var SOEmarker = L.marker([10.04997, 76.33064]).addTo(mymap);
        SOEmarker.bindPopup("<b>SOE,CUSAT</b><br>School of Engineering, CUSAT<br> <a href=\"https://goo.gl/maps/YgViUUFkceFEA69E9\">Show in Maps</a>");
        
        
        var FrontDeskmarker = L.marker([10.04980, 76.3309],{icon : dhishnaIcon }).addTo(mymap);
        FrontDeskmarker.bindPopup("<b>DHISHNA 2020 Front Desk</b><br>School of Engineering, CUSAT<br> <a href=\"https://goo.gl/maps/YgViUUFkceFEA69E9\">Show in Maps</a>");
        var Seminarmarker = L.marker([10.04392, 76.32410]).addTo(mymap);
        Seminarmarker.bindPopup("<b>Seminar Complex</b><br>CUSAT <br> <a href=\"https://goo.gl/maps/HFTUt1reMar1TFBa7\">Show in Maps</a>");
    </script>

